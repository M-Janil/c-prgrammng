#include<stdio.h>
int main()
{
    int i,j,t=0,x,k,a;
    printf("enter the number of elements in array");
    scanf("%d",&a);
    int arr[a],b[a];
    for(i=0;i<a;i++)
    {
        printf("arr[%d]",i);
        scanf("%d",&arr[i]);
        b[i]=arr[i];
    }
    for(i=0;i<a;i++)
    {
        for(j=i+1;j<a;j++)
        {
            if(arr[i]==b[j])
            {
                for(x=j;x<a-1;x++)
            {
                b[x]=b[x+1];
            }
            a--;
            }
        }
        
    }
    for(i=0;i<a;i++)
    {
        printf("%d",b[i]);
    }
    return 0;
    
}
