#include <stdio.h>
int fact(int n)
{
    int fact=1;
    if(n==0)
    return n;
    else if(n==1)
    return 1;
    else
    {
        fact=fact*fact(n-1);
    }
    return fact;
}
int factt(int n)
{
    int fact=1,i;
    if(n==0)
    return 1;
    else 
    {
        for(i=n;i>0;i--)
        {
        fact=fact*i;
    }
    return fact;
}
