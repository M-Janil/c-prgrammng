#include<stdio.h>
int main()
{
    int n, j=0,i=0;
    printf("enter the number of elements to be sorted:\n");
    scanf("%d",&n);
    int arr[n],temp=0;
    for(i=0;i<n;i++)
    {
        printf("enter the arr[%d]\n",i);
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++)
    {
        temp=0;
        for(j=0;j<n-i-1;j++)
        {
            if(arr[j]>arr[j+1])
            {
                temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
            }
        }
    }
    for(i=0;i<n;i++)
    {
        printf("sorted order=%d\n",arr[i]);
    }
    return 0;
}
