#include<stdio.h>
int main()
{
  int r,c,i,j,temp=0;
  printf("enter the rows ad clums");
  scanf("%d %d",&r,&c);
  int arr[r][c],b[r][c];
  for(i=0;i<r;i++)
  {
      for(j=0;j<c;j++)
      {
          printf("b");
          scanf("%d",&arr[i][j]);
      }
  }
  for(i=0;i<r;i++)
  {
      for(j=0;j<c;j++)
      {
          b[i][j]=arr[i][c-1-j];
      }
  }
  for(i=0;i<r;i++)
  {
      for(j=0;j<c;j++)
      {
          printf("%d \t",b[i][j]);
      }
      printf("\n");
  }
  return 0;
}
