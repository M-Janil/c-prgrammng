#include <stdio.h>
#include <string.h>

int main()
{
    char s1[100], s2[100];
    int count[256] = {0}, i;

    printf("Enter string 1: ");
    scanf("%s", s1);

    printf("Enter string 2: ");
    scanf("%s", s2);

    if(strlen(s1) != strlen(s2))
    {
        printf("Not an anagram");
        return 0;
    }

    for(i = 0; s1[i]; i++)
        count[s1[i]]++;

    for(i = 0; s2[i]; i++)
        count[s2[i]]--;

    for(i = 0; i < 256; i++)
    {
        if(count[i] != 0)
        {
            printf("Not an anagram");
            return 0;
        }
    }

    printf("Anagram");
    return 0;
}
