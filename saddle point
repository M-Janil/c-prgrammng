#include<stdio.h>
int main()
{
    int r,c,i,j,temp1=0,temp2=0,k,n,x,y;
    printf("enter the no of rows of rows and columns in order");
    scanf("%d %d",&r,&c);
    int arr[r][c];
    for(i=0;i<r;i++)
        {
            for(j=0;j<c;j++)
            {
                printf("arr[%d][%d]=",i,j);
                scanf("%d",&arr[i][j]);
            }
        }
    for(i=0;i<r;i++)
    {
        temp1=arr[i][0];
        x=0;
        for(k=0;k<c;k++)
        {
            if(arr[i][k]<temp1)
            {
                temp1=arr[i][k];
                x=k;
            }
        }
    temp2=1;
        for(j=0;j<c;j++)
        {
            if(arr[j][x]>temp1)
            { temp2=0;
            break;
        }
        }
    if(temp2==1)
    {
        printf("saddle point at arr[%d][%d]=%d",i,x,arr[i][x]);
    }
    else
    {printf("not a saddle point");}}
    return 0;
}
